\proj reset 
\PREFERENCES SET ENERGY_SCALE KINETIC
\PREFERENCES SET PLOT_ZERO 1.000000E-009
-- put all kind of materials in system
\SAMPLE SET MATERIAL (/SI[SIO2]/O2/)81(/B2/O3/)13(/AL2/O3/)2(/NA2/O/)4(/SI/NB/SI[X]/NB[X]/)1 LAYER 1
-- set bottom layers
\SAMPLE ADD LAYER (/SI[SIO2]/O2/)81(/B2/O3/)13(/AL2/O3/)2(/NA2/O/)4 THICKNESS 200 ABOVE 0
-- 
-- #11-15 
\SAMPLE ADD LAYER /SI[SIO2]/O2/ THICKNESS 15 ABOVE 0
\SAMPLE ADD LAYER /SI[SI2O3]2/O3/ THICKNESS 10 ABOVE 0
\SAMPLE ADD LAYER /SI[SI2O]2/O/ THICKNESS 5 ABOVE 0
\SAMPLE ADD LAYER /SI/ THICKNESS 70 ABOVE 0
\SAMPLE ADD LAYER /SI7/SI[X]/NB[X]8/  THICKNESS 0.0001 ABOVE 0
-- 
-- #7-10 
\SAMPLE ADD LAYER /SI7/SI[X]/NB[X]8/  THICKNESS 0.0001 ABOVE 0
\SAMPLE ADD LAYER /SI[X]0.1/NB[X]0.9/  THICKNESS 0.0001 ABOVE 0
\SAMPLE ADD LAYER /NB/  THICKNESS 13 ABOVE 0
\SAMPLE ADD LAYER /SI[X]0.1/NB[X]0.9/  THICKNESS 0.0001 ABOVE 0
-- 
-- #1-6
\SAMPLE ADD LAYER /SI7/SI[X]/NB[X]8/  THICKNESS 0.0001 ABOVE 0
\SAMPLE ADD LAYER /SI/NB/  THICKNESS 0.0001 ABOVE 0
\SAMPLE ADD LAYER /SI/ THICKNESS 60 ABOVE 0
\SAMPLE ADD LAYER /SI[SI2O]2/ THICKNESS 0.0001 ABOVE 0
\SAMPLE ADD LAYER /SI[SI2O]10/SI[SI2O3]10/SI[SIO2]14/SI66/ THICKNESS 0.0001 ABOVE 0
\SAMPLE ADD LAYER /SI[SIO2]29/SI[SI2O3]4/SI[SI2O]0.1/SI33/ THICKNESS 40 ABOVE 0

-- from Up to down
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/SI/O2/" LAYER 1
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/SI/O2/" LAYER 2
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/SI/O2/" LAYER 3
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/SI/" LAYER 4
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/SI/" LAYER 5
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/SI/" LAYER 6
-- 
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/NB/" LAYER 7
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/NB/" LAYER 8
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/NB/" LAYER 9
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/NB/" LAYER 10
-- 
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/NB/" LAYER 11
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/SI/" LAYER 12
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/SI/O2/" LAYER 13
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/SI/O2/" LAYER 14
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/SI/O2/" LAYER 15
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/SI/O2/" LAYER 16
\SAMPLE PARAMETERS SET DIIMFP MATERIAL "/SI/O2/" LAYER 17

\SPECTROMETER SET RANGE 1370:1398 REGION 1
\SPECTROMETER ADD REGION RANGE 1270:1290

--
\SAMPLE PEAK DELETE PEAK 13
\SAMPLE PEAK DELETE PEAK 11
\SAMPLE PEAK DELETE PEAK 9
\SAMPLE PEAK DELETE PEAK 7
\SAMPLE PEAK DELETE PEAK 5
\SAMPLE PEAK DELETE PEAK 3
\SAMPLE PEAK DELETE PEAK 1

\SAMPLE PEAK LOAD DATA "casaXPSspectra\ref_Si2p_SiO2.dat" SUBPEAK 1 PEAK 1
\SAMPLE PEAK LOAD DATA "casaXPSspectra\ref_Si2p_Si-Si.dat" SUBPEAK 1 PEAK 2
\SAMPLE PEAK LOAD DATA "casaXPSspectra\ref_Si2p_Si-Nb.dat" SUBPEAK 1 PEAK 3
\SAMPLE PEAK LOAD DATA "casaXPSspectra\ref_Si2p_Si2O3.dat" SUBPEAK 1 PEAK 4
\SAMPLE PEAK LOAD DATA "casaXPSspectra\ref_Si2p_Si2O.dat" SUBPEAK 1 PEAK 5
\SAMPLE PEAK LOAD DATA "casaXPSspectra\ref_Nb3d_Nb-Nb.dat" SUBPEAK 1 PEAK 6
\SAMPLE PEAK LOAD DATA "casaXPSspectra\ref_Nb3d_Nb-Si.dat" SUBPEAK 1 PEAK 7

--
\PREFERENCES SET NDIIMFP 2
\MODEL SET CONVERGENCE 1.000e-003
\GEOMETRY SET SOURCE THETA 45 GEOMETRY 1
\GEOMETRY SET ANALYZER THETA 45 GEOMETRY 1
\GEOMETRY SET APERTURE UTHETA 20 GEOMETRY 1
\SPECTROMETER SET RANGE 1380:1392 REGION 1
\SPECTROMETER SET RANGE 1278:1288 REGION 2

